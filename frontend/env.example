# AWS S3 Configuration for Direct Uploads
NEXT_PUBLIC_AWS_ACCESS_KEY_ID=your_access_key_id
NEXT_PUBLIC_AWS_SECRET_ACCESS_KEY=your_secret_access_key
NEXT_PUBLIC_AWS_REGION=us-west-2
NEXT_PUBLIC_S3_BUCKET=patent-novelty-pdf-processing-YOUR_ACCOUNT_ID

# DynamoDB Configuration
NEXT_PUBLIC_DYNAMODB_TABLE=patent-keywords-YOUR_ACCOUNT_ID

# Patent Search Results Table
NEXT_PUBLIC_PATENT_SEARCH_RESULTS_TABLE=patent-search-results-YOUR_ACCOUNT_ID

# Scholarly Articles Results Table
NEXT_PUBLIC_SCHOLARLY_RESULTS_TABLE_NAME=scholarly-articles-results-YOUR_ACCOUNT_ID

# Bedrock Agent Core Configuration
NEXT_PUBLIC_BEDROCK_AGENT_RUNTIME_ARN=arn:aws:bedrock-agentcore:us-west-2:YOUR_ACCOUNT_ID:runtime/PatentNovelty-xxxxxxxx

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=false

